{% extends 'base.html.twig' %}

{% block title %}Resultado da Pesquisa{% endblock %}

{% block body %}
    <h1>Resultado da Pesquisa</h1>

    {% if cidadao is not null %}
        <p>Nome: {{ cidadao.getNome() }}</p>
        <p>NIS: {{ cidadao.getNis() }}</p>
    {% else %}
        <p>Cidadão não encontrado.</p>
    {% endif %}
{% endblock %}
